snippet     assertEqual
    assert.Equal(t, ${1:EXPECTED}, ${1:TARGET})

snippet     assertNotEqual
    assert.NotEqual(t, ${1:EXPECTED}, ${1:TARGET})

snippet     assertNil
    assert.Nil(t, ${1:TARGET})

snippet     assertNotNil
    assert.NotNil(t, ${1:TARGET})

snippet     dump
	fmt.Printf("${1:TARGET} = %#v\\n", $1)
